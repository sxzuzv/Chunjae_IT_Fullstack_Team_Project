<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="repository.mapper.StudentClassMapper_jh">
    <select id="getAllMyClass" resultType="int">
        select class_id
        from student_class
        where student_id = #{studentIdx}
    </select>
</mapper>